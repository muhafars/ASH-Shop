{
  "name": "ashshop",
  "version": "1.0.0",
  "description": "ECommerce app built with MERN Stack",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "node backend/app.js",
    "server": "nodemon backend/app.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muhafars/ASH-Shop.git"
  },
  "author": "Muhammad Fajri Assidiq a.k muhafars",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muhafars/ASH-Shop/issues"
  },
  "homepage": "https://github.com/muhafars/ASH-Shop#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.1"
  }
}
